<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yan.mybatis.dao.AppraisalAddressMapper">

    <select id="selectById" parameterType="java.lang.Long" resultType="com.yan.mybatis.po.AppraisalAddress">
        SELECT * FROM `appraisaladdress`
        where id = #{id}
    </select>

    <select id="queryUserList" parameterType="java.lang.String" resultType="com.yan.mybatis.po.AppraisalAddress">
        SELECT * FROM `appraisaladdress`
        where telephone = #{telephone}
    </select>

</mapper>